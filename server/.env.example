OPENAI_API_KEY=your_openai_api_key_here
REDIS_URL=redis://localhost:6379
PORT=5000
NODE_ENV=development

# OpenAI Configuration (Optional - defaults provided)
# Model: 'gpt-4-turbo' for best quality, 'gpt-3.5-turbo' for cost efficiency
OPENAI_MODEL=gpt-4-turbo
OPENAI_TEMPERATURE=0.3
OPENAI_MAX_TOKENS=3000
OPENAI_PRESENCE_PENALTY=0.1
OPENAI_FREQUENCY_PENALTY=0.1

# OpenAI Web Search Configuration (Optional - defaults provided)
# Enable/disable web search functionality
OPENAI_WEB_SEARCH_ENABLED=true
# Web search context size: 'high' (best quality, slower, more expensive), 'medium' (balanced), 'low' (fast, cheaper)
OPENAI_WEB_SEARCH_CONTEXT_SIZE=medium